web:
  image: docker.io/avatarnewyork/dockerenv-apache:$DOCKERENV_APACHE_TAG
  volumes:
    - "$VOLUME_MOUNT:$WEB_ROOT"
  ports:
    - "8080:80"
  links:
    - db
  environment:
    POSTFIX_HOSTNAME: dev.avatarnewyork.com
db:
  image: docker.io/avatarnewyork/dockerenv-mysql:$DOCKERENV_MYSQL_TAG
  ports:
    - "3366:3306"
    
